<br/>
<div class="col-lg-12 col-lg-offset-1">
  <h2 class="welcome-message">Dashboard</h2>

  <div class="row">
    <div class="col-lg-6 col-lg-offset-3">
      <div class="well">
          
          <div class="row">
            <div class="col-lg-6 col-lg-offset-3">
               <h2 class="welcome-message" style="color: #000;">Apply Filters:</h2> 
            </div>
          </div>

          <div class="row">
            
            <div class="col-lg-5">
                % Criteria<select name="test" id="test">
                            <option>% throughout</option>
                            <option value="75">> 75</option>
                            <option value="70">> 70</option>
                            <option value="65">> 65</option>
                            <option value="60">> 60</option>
                            <option value="55">> 55</option>
                            <option value="50">> 50</option>
                          </select>

            </div>

            <div class="col-lg-7">
               Back Criteria<select name="back" id="back">
                              <option>Backs throughout</option>
                              <option value="0">None</option>
                              <option value="1">< 1</option>
                              <option value="2">< 2</option>
                              <option value="3">< 3</option>
                              <option value="4">< 4</option>
                              <option value="5">< 5</option>
                            </select>
            </div><hr/>
            <div class="row">
              <div class="col-lg-6 col-lg-offset-3">
                <button id="filter" class="btn btn-danger">Filter</button>     
              </div>
            </div>
            
          </div>
      </div>
    </div>
  </div>   

       <div class="row">
    <div class="col-lg-4 col-lg-offset-4">
      <button id="export" class="btn btn-lg btn-info">Export to Excel</button>
    </div>
  </div> 
    

<br/>
<hr/> 
<div class="well">
{{#if students}}
<table class="table table-striped" id="mytable">
  <thead>
    <tr>
      <th scope="col">S.No.</th>
      <th scope="col">Roll No.</th>
      <th scope="col">College ID</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Gender</th>
      <th scope="col">Email</th>
      <th scope="col">Phone No.</th>
      <th scope="col">10 Score</th>
      <th scope="col">12 Score</th>
      <th scope="col">B.Tech. Score</th>
      <th scope="col">Backs</th>
    </tr>  
  </thead>
  <div id="div1">
  <tbody id="tbody" name="tbody">
 
  	{{#each students}}
  		<tr>
    		<td>{{inc @index}}</td>
    		<td>{{rollNO}}</td> 
    		<td>{{collegeID}}</td>
    		<td>{{first_name}}</td>
    		<td>{{last_name}}</td>
        <td>{{gender}}</td>
    		<td>{{email}}</td>
        <td>{{phone}}</td>
    		<td>{{tenthMarks}}</td>
    		<td>{{twelvthMarks}}</td>
        <td>{{btechMarks}}</td>
        <td>{{backlogs}}</td>
  		</tr>
  	{{/each}}
  {{else}}
  <td>No data to display!</td>
  {{/if}}	
  </tbody>
  </div>	
</table>
</div>
<hr/>
</div>

</div>
</div>



          